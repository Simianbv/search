{
  "name": "simianbv/search",
  "description": "A utility package that works on top of laravel",
  "type": "project",
  "keywords": [
    "laravel",
    "search"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Simian",
      "email": "it@simian.nl"
    }
  ],
  "require": {
    "php": ">=7.4|^8.0|^8.1",
    "laravel/framework" : "^8.0|^9.0",
    "simianbv/introspect": "2.*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Simianbv\\Search\\": "src/"
    },
    "files": [
      "src/Helpers/cache.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Simianbv\\Search\\SearchServiceProvider"
      ]
    }
  },
  "minimum-stability": "beta"
}





